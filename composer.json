{
    "name": "onedesign/craft-bugherd",
    "description": "Easily install Bugherd on your Craft CMS site",
    "type": "craft-plugin",
    "version": "1.0.0",
    "keywords": [
        "craft",
        "cms",
        "craftcms",
        "craft-plugin",
        "bugherd"
    ],
    "support": {
        "docs": "https://github.com/onedesign/bugherd/blob/master/README.md",
        "issues": "https://github.com/onedesign/bugherd/issues"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "One Design Company",
            "homepage": "https://onedesigncompany.com/"
        }
    ],
    "require": {
        "craftcms/cms": "^3.5.0"
    },
    "require-dev": {
      "craftcms/cms": "^3.2.0",
      "codeception/codeception": "^3.0",
      "vlucas/phpdotenv": "^3.0"
    },
    "autoload": {
        "psr-4": {
          "onedesign\\bugherd\\": "src/"
        }
    },
    "extra": {
        "name": "Bugherd",
        "handle": "bugherd",
        "developer": "One Design Company",
        "developerUrl": "https://onedesigncompany.com/",
        "documentationUrl": "https://github.com/onedesign/bugherd/blob/master/README.md",
        "changelogUrl": "https://raw.githubusercontent.com/onedesign/bugherd/master/CHANGELOG.md",
        "class": "onedesign\\bugherd\\Bugherd"
    }
}
